# Node environment
NODE_ENV=development
PORT=5000

# Auth0 Configuration
AUTH0_DOMAIN=your-auth0-domain.auth0.com
AUTH0_AUDIENCE=https://your-api-identifier
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret

# Auth0 Role IDs
AUTH0_ROLE_ADMIN=rol_abc123
AUTH0_ROLE_VOLUNTEER=rol_def456
AUTH0_ROLE_PARTICIPANT=rol_ghi789

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=community_care
DB_USER=postgres
DB_PASSWORD=your-password

# Snowflake API Configuration
SNOWFLAKE_API_KEY=your-snowflake-api-key
SNOWFLAKE_ENDPOINT=https://your-snowflake-endpoint.cloud/api/v1/query

# Logging Configuration
LOG_LEVEL=debug